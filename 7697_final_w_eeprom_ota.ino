  mcs.process(100);

  if (current_status.updated()) {
    int receive_data = current_status.value();
    Serial.print("控制通道更新 :");
    Serial.println(receive_data);
    change_lcd_status(receive_data);

  }

  while (!mcs.connected()) {
    Serial.println("re-connect to MCS...");
    mcs.connect();
    if (mcs.connected())
      Serial.println("MCS connected !!");
  }

}


